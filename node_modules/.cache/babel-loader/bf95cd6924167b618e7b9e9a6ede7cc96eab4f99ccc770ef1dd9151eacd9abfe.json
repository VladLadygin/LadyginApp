{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport draggable from \"@/vuedraggable\";\nexport default {\n  name: \"two-lists\",\n  display: \"Two Lists\",\n  order: 1,\n  components: {\n    draggable\n  },\n  data() {\n    return {\n      list1: [{\n        name: \"John\",\n        id: 1\n      }, {\n        name: \"Joao\",\n        id: 2\n      }, {\n        name: \"Jean\",\n        id: 3\n      }, {\n        name: \"Gerard\",\n        id: 4\n      }],\n      list2: [{\n        name: \"Juan\",\n        id: 5\n      }, {\n        name: \"Edgard\",\n        id: 6\n      }, {\n        name: \"Johnson\",\n        id: 7\n      }]\n    };\n  },\n  methods: {\n    add: function () {\n      this.list.push({\n        name: \"Juan\"\n      });\n    },\n    replace: function () {\n      this.list = [{\n        name: \"Edgard\"\n      }];\n    },\n    clone: function (el) {\n      return {\n        name: el.name + \" cloned\"\n      };\n    },\n    log: function (evt) {\n      window.console.log(evt);\n    }\n  }\n};","map":{"version":3,"names":["draggable","name","display","order","components","data","list1","id","list2","methods","add","list","push","replace","clone","el","log","evt","window","console"],"sources":["/home/vlad12345/Projects/aaa.ru/src/components/HelloWorld.vue"],"sourcesContent":["<template>\n  <div class=\"row\">\n    <div class=\"col-3\">\n      <h3>Draggable 1</h3>\n      <draggable\n        class=\"list-group\"\n        :list=\"list1\"\n        group=\"people\"\n        @change=\"log\"\n        itemKey=\"name\"\n      >\n        <template #item=\"{ element, index }\">\n          <div class=\"list-group-item\">{{ element.name }} {{ index }}</div>\n        </template>\n      </draggable>\n    </div>\n\n    <div class=\"col-3\">\n      <h3>Draggable 2</h3>\n      <draggable\n        class=\"list-group\"\n        :list=\"list2\"\n        group=\"people\"\n        @change=\"log\"\n        itemKey=\"name\"\n      >\n        <template #item=\"{ element, index }\">\n          <div class=\"list-group-item\">{{ element.name }} {{ index }}</div>\n        </template>\n      </draggable>\n    </div>\n\n    <rawDisplayer class=\"col-3\" :value=\"list1\" title=\"List 1\" />\n\n    <rawDisplayer class=\"col-3\" :value=\"list2\" title=\"List 2\" />\n  </div>\n</template>\n<script>\nimport draggable from \"@/vuedraggable\";\n\nexport default {\n  name: \"two-lists\",\n  display: \"Two Lists\",\n  order: 1,\n  components: {\n    draggable\n  },\n  data() {\n    return {\n      list1: [\n        { name: \"John\", id: 1 },\n        { name: \"Joao\", id: 2 },\n        { name: \"Jean\", id: 3 },\n        { name: \"Gerard\", id: 4 }\n      ],\n      list2: [\n        { name: \"Juan\", id: 5 },\n        { name: \"Edgard\", id: 6 },\n        { name: \"Johnson\", id: 7 }\n      ]\n    };\n  },\n  methods: {\n    add: function() {\n      this.list.push({ name: \"Juan\" });\n    },\n    replace: function() {\n      this.list = [{ name: \"Edgard\" }];\n    },\n    clone: function(el) {\n      return {\n        name: el.name + \" cloned\"\n      };\n    },\n    log: function(evt) {\n      window.console.log(evt);\n    }\n  }\n};\n</script>"],"mappings":";AAsCA,OAAOA,SAAQ,MAAO,gBAAgB;AAEtC,eAAe;EACbC,IAAI,EAAE,WAAW;EACjBC,OAAO,EAAE,WAAW;EACpBC,KAAK,EAAE,CAAC;EACRC,UAAU,EAAE;IACVJ;EACF,CAAC;EACDK,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,KAAK,EAAE,CACL;QAAEL,IAAI,EAAE,MAAM;QAAEM,EAAE,EAAE;MAAE,CAAC,EACvB;QAAEN,IAAI,EAAE,MAAM;QAAEM,EAAE,EAAE;MAAE,CAAC,EACvB;QAAEN,IAAI,EAAE,MAAM;QAAEM,EAAE,EAAE;MAAE,CAAC,EACvB;QAAEN,IAAI,EAAE,QAAQ;QAAEM,EAAE,EAAE;MAAE,EACzB;MACDC,KAAK,EAAE,CACL;QAAEP,IAAI,EAAE,MAAM;QAAEM,EAAE,EAAE;MAAE,CAAC,EACvB;QAAEN,IAAI,EAAE,QAAQ;QAAEM,EAAE,EAAE;MAAE,CAAC,EACzB;QAAEN,IAAI,EAAE,SAAS;QAAEM,EAAE,EAAE;MAAE;IAE7B,CAAC;EACH,CAAC;EACDE,OAAO,EAAE;IACPC,GAAG,EAAE,SAAAA,CAAA,EAAW;MACd,IAAI,CAACC,IAAI,CAACC,IAAI,CAAC;QAAEX,IAAI,EAAE;MAAO,CAAC,CAAC;IAClC,CAAC;IACDY,OAAO,EAAE,SAAAA,CAAA,EAAW;MAClB,IAAI,CAACF,IAAG,GAAI,CAAC;QAAEV,I